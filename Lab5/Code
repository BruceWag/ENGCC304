#include <stdio.h>

int main() {
    int score = 0 ;
    int validInput = 0 ;
    
    while (!validInput) {
        printf("Enter your score : ") ;
        if (scanf("%d", &score) != 1) {
            printf("Please enter number only \n") ;
            while (getchar() != '\n') ;
        } else {
            validInput = 1 ;
        }
    }
    
    printf("Grade : ") ;
    if(score >= 90 ) {
        printf("A") ; 
    } else if (score >= 75) {
        printf("B+") ; 
    } else if (score >= 70) {
        printf("B") ; 
    } else if (score >= 65) {
        printf("C+") ;
    } else if (score >= 60) {
        printf("C") ;
    } else if (score >= 55) {
        printf("D+") ;
    } else if (score >= 50) {
        printf("D") ;
    } else {
        printf("F") ;
    }
    return 0 ;
}
